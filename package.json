    "bcrypt": "^5.1.0",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.3.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.11.9",
    "babel-plugin-module-resolver": "^4.1.0",
    "prisma": "^4.6.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
